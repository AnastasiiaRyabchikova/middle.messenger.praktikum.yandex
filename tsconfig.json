{
  "outDir": "dist",
  "target": "es2016",
  "declaration": false,
  "module": "commonjs",
  "noImplicitAny": false,
  "noUnusedLocals": true,
  "noUnusedParameters": true,
  "sourceMap": true,
  "strictNullChecks": false,
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "templator": ["./src/templator"],
      "classnames": ["./src/utils/classnames"],
      "~*": ["./*"]
    }
  }
}
